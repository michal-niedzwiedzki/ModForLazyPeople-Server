{
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "useTabs": false,
  "tabWidth": 2,
  "semi": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "printWidth": 120,
  "importOrder": ["^[a-zA-Z](.*)$", "^@[a-zA-Z](.*)$$", "^[./]"],
  "importOrderSeparation": true
}
